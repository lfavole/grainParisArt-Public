{
    "buildCommand": "bash build.sh",
    "crons": [
        {
          "path": "/api/fetch_data",
          "schedule": "0 1 * * *"
        }
    ],
    "outputDirectory": "static",
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/$1"
        },
        {
            "source": "/(.*)",
            "destination": "api/app.py"
        }
    ]
}
